#include <iostream>
#include <string>
using namespace std;
int main ( ) {
	string nama = "Wirda hayati";
	string nim = "ETE10190122";
	cout << endl;
	cout << "~~~~~~~~~~~~~~SELASA, 07 APRIL 2020~~~~~~~~~~~~~~";
	cout << endl;
	cout << "Nama : " << nama << endl;
	cout << "NIM : " << nim << endl;
	cout << endl;
	//Membuat Program Break
	for (int i = 1; i <= 200; i++)
	{
		if (i > 180) break;
		cout << i << endl;
	}
	return 0;
}
