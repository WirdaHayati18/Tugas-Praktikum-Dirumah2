	string nim = "ETE10190122";
	cout << endl;
	cout << "~~~~~~~~~~~~~~SELASA, 07 April 2020~~~~~~~~~~~~~~";
	cout << endl;
	cout << "Nama : " << nama << endl;
	cout << "NIM : " << nim;
	cout << endl;
	cout << endl;
	//Membuat Program Continue
	for (int i = 1; i <= 200; i++)
	{
		if (i % 2 == 1) continue;
		cout << i << endl;
	}
	return 0;
}
